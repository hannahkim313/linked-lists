{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "import/extensions": [
        "error",
        {
          "js": "ignorePackages"
        }
      ]
    }
}
